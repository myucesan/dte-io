<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import javafx.scene.control.TextField?>
<VBox fx:id="vb" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.deltaproto.PrimaryController">
   <children>
      <Label fx:id="connectedStatus" text="Not connected" />
      <Button fx:id="connectButton" text="Connect to WebSocket" onAction="#connectToServer"/>
      <Label fx:id="pedalLabel" text="Not pressed" />
      <TextField fx:id="messageTB"/>
      <Button fx:id="sendMessage" text="SendMessage" onAction="#sendMessage" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
